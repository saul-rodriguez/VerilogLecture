#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xe60730 .scope module, "count4_tb" "count4_tb" 2 3;
 .timescale 0 0;
v0xea5c00_0 .var "Clock", 0 0;
v0xea5cd0_0 .var "E", 0 0;
v0xea5d80_0 .net "Q", 3 0, v0xea5ab0_0; 1 drivers
v0xea5e30_0 .var "Resetn", 0 0;
S_0xe60820 .scope module, "my_count4" "count4" 2 30, 3 3, S_0xe60730;
 .timescale 0 0;
v0xe95e30_0 .net "Clock", 0 0, v0xea5c00_0; 1 drivers
v0xea5a10_0 .net "E", 0 0, v0xea5cd0_0; 1 drivers
v0xea5ab0_0 .var "Q", 3 0;
v0xea5b50_0 .net "Resetn", 0 0, v0xea5e30_0; 1 drivers
E_0xe72360/0 .event negedge, v0xea5b50_0;
E_0xe72360/1 .event posedge, v0xe95e30_0;
E_0xe72360 .event/or E_0xe72360/0, E_0xe72360/1;
    .scope S_0xe60820;
T_0 ;
    %wait E_0xe72360;
    %load/v 8, v0xea5b50_0, 1;
    %mov 9, 0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/0xz  T_0.0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0xea5ab0_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0xea5a10_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v0xea5ab0_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v0xea5ab0_0, 0, 8;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0xe60730;
T_1 ;
    %vpi_call 2 12 "$dumpfile", "count4.vcd";
    %vpi_call 2 13 "$dumpvars", 1'sb0, S_0xe60820;
    %set/v v0xea5e30_0, 1, 1;
    %set/v v0xea5cd0_0, 0, 1;
    %set/v v0xea5c00_0, 0, 1;
    %delay 2, 0;
    %set/v v0xea5e30_0, 0, 1;
    %delay 2, 0;
    %set/v v0xea5e30_0, 1, 1;
    %delay 2, 0;
    %set/v v0xea5cd0_0, 1, 1;
    %delay 200, 0;
    %vpi_call 2 22 "$finish";
    %end;
    .thread T_1;
    .scope S_0xe60730;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0xea5c00_0, 1;
    %inv 8, 1;
    %set/v v0xea5c00_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "count4_tb.v";
    "count4.v";
